{
  "mcpServers": {
    "supabase-mcp-server": {
      "command": "npx",
      "args": [
        "-y",
        "@supabase/mcp-server-supabase@latest",
        "--access-token",
        "YOUR_SUPABASE_ACCESS_TOKEN_HERE"
      ],
      "env": {},
      "disabled": false,
      "autoApprove": [
        "mcp_supabase_mcp_server_list_projects",
        "mcp_supabase_mcp_server_get_project",
        "mcp_supabase_mcp_server_list_tables",
        "mcp_supabase_mcp_server_execute_sql",
        "mcp_supabase_mcp_server_search_docs",
        "list_projects",
        "get_project",
        "list_tables",
        "execute_sql"
      ]
    },
    "github-mcp-server": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GITHUB_PERSONAL_ACCESS_TOKEN",
        "ghcr.io/github/github-mcp-server"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
      },
      "disabled": true,
      "autoApprove": []
    },
    "fetch": {
      "command": "uvx",
      "args": [
        "mcp-server-fetch"
      ],
      "env": {},
      "disabled": true,
      "autoApprove": []
    }
  }
}